				<textElement textAlignment="Justified" markup="styled">
					<font fontName="Calibri" size="10" isPdfEmbedded="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA["I have read, understood and agree to the terms and conditions as set forth in these Terms of Employment. I accept the offer of employment on the terms and conditions mentioned in the above letter."]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="17" y="705" width="59" height="15"/>
				<textElement>
					<font fontName="Calibri" size="10" isBold="false" isPdfEmbedded="true"/>
				</textElement>

	<queryString>
		<![CDATA[SELECT FIRST_NAME,LAST_NAME,DATE_OF_JOINING ,DESIGNATION,LABEL,SUM
FROM CANDIDATE C
LEFT JOIN PROFILE_INFO PI ON PI.ID = C.PRO_APPROVAL_ID
LEFT JOIN LEVELS L ON PI.LEVEL_ID = L.ID
LEFT JOIN SALARY_FOR_APPROVAL SD ON SD.CANDIDATE_ID = C.ID
WHERE C.ID=$P{CANDIDATE_ID} AND SD.SUM IS NOT NULL]]>
	</queryString>

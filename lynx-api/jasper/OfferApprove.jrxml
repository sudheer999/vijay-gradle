	<queryString>
		<![CDATA[SELECT FIRST_NAME,LAST_NAME,DATE_OF_JOINING ,DESIGNATION,LABEL,NAME,SI.*,CPD.*,ADR.ADDRESS,ADR.CITY,ZIPCODE,FIXED_PERDIEM,SI.PERDIEM_OFFER_STRING,SI.RELOCATION_PLACE
FROM CANDIDATE C
LEFT JOIN PROFILE_INFO PI ON PI.ID = C.PRO_APPROVAL_ID
LEFT JOIN PERSONAL_INFO PER ON PER.ID = C.PER_APPROVAL_ID
LEFT JOIN ADDRESS ADR ON ADR.ID = PER.PERMANENT_ADDRESS
LEFT JOIN LEVELS L ON PI.LEVEL_ID = L.ID
LEFT JOIN DIVISON D ON L.DIVISION_ID = D.ID
LEFT JOIN SALARY_INFO_FOR_APPROVAL SI ON SI.BASIC=C.ID
LEFT JOIN CANDIDATE_PERDIEM_DETAILS CPD ON CPD.CANDIDATE_ID=C.ID
LEFT JOIN FIXED_PERDIEM_CANDIDATE FPD ON FPD.ID=C.ID
WHERE C.ID=$P{CANDIDATE_ID}]]>
	</queryString>
